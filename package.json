{
  "name": "react-video-packager",
  "version": "2.0.0",
  "description": "React Video Packager",
  "main": "./src/scripts/index.js",
  "author": "maximobelen",
  "private": true,
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.html ./dist",
    "assets": "cp -R ./src/images ./dist",
    "dist": "npm run copy & webpack --env=dist & npm run assets",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "start": "node server.js --env=dev",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(s?css|less)$": "identity-obj-proxy"
    }
  },
  "repository": "",
  "keywords": [],
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^6.0.0",
    "babel-jest": "^22.4.4",
    "babel-loader": "^6.0.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.5.0",
    "bower-webpack-plugin": "^0.1.9",
    "chai": "^3.2.0",
    "copy-dir": "^0.3.0",
    "copyfiles": "^1.0.0",
    "css-loader": "^0.23.0",
    "eslint": "^3.0.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^6.0.0",
    "file-loader": "^0.9.0",
    "glob": "^7.0.0",
    "isparta-instrumenter-loader": "^1.0.0",
    "jest": "^22.4.4",
    "minimist": "^1.2.0",
    "mocha": "^3.0.0",
    "node-sass": "^4.5.3",
    "null-loader": "^0.1.1",
    "open": "0.0.5",
    "phantomjs-prebuilt": "^2.0.0",
    "react-hot-loader": "^1.2.9",
    "react-test-renderer": "^16.3.2",
    "rimraf": "^2.4.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.15.0",
    "webpack-dev-server": "^1.12.0"
  },
  "dependencies": {
    "core-js": "^2.0.0",
    "fbemitter": "^2.1.1",
    "flux": "^3.1.3",
    "is-mobile": "^0.2.2",
    "load-svg": "^1.0.0",
    "node-sass": "^4.5.3",
    "normalize.css": "^4.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.0.0",
    "react-addons-css-transition-group": "^15.6.0",
    "react-addons-transition-group": "^15.6.0",
    "react-bootstrap": "^0.31.2",
    "react-dom": "^15.6.1",
    "react-player-controls": "^0.5.20",
    "sass-loader": "^6.0.6",
    "superagent": "^3.8.1",
    "webpack": "^1.15.0"
  }
}
